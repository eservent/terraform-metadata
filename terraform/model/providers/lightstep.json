{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v2",
  "name": "lightstep",
  "type": "provider",
  "version": "v1.62.0",
  "provider": {
    "api_key": {
      "Type": "String",
      "Optional": true,
      "Description": "The API Key for a Lightstep organization."
    },
    "api_key_env_var": {
      "Type": "String",
      "Optional": true,
      "Description": "Environment variable for Lightstep api key.",
      "Default": {
        "Type": "string",
        "Value": "LIGHTSTEP_API_KEY"
      }
    },
    "environment": {
      "Type": "String",
      "Optional": true,
      "Description": "The name of the Lightstep environment, must be one of: staging, meta, public.",
      "DefaultFunc": "ENV(LIGHTSTEP_ENV)"
    },
    "organization": {
      "Type": "String",
      "Required": true,
      "Description": "The name of the Lightstep organization",
      "DefaultFunc": "ENV(LIGHTSTEP_ORG)"
    }
  },
  "resources": {
    "lightstep_alerting_rule": {
      "condition_id": {
        "Type": "String",
        "Required": true
      },
      "destination_id": {
        "Type": "String",
        "Required": true
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "update_interval": {
        "Type": "String",
        "Required": true
      }
    },
    "lightstep_metric_condition": {
      "alerting_rule": {
        "Type": "List",
        "Optional": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "exclude_filters": {
              "Type": "List",
              "Optional": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "Map"
              }
            },
            "filters": {
              "Type": "List",
              "Optional": true,
              "Description": "Non-equality filters (operand: contains, regexp, etc)",
              "Computed": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "Map"
              }
            },
            "id": {
              "Type": "String",
              "Required": true
            },
            "include_filters": {
              "Type": "List",
              "Optional": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "Map"
              }
            },
            "update_interval": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "description": {
        "Type": "String",
        "Optional": true
      },
      "expression": {
        "Type": "List",
        "Required": true,
        "MaxItems": 1,
        "MinItems": 1,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "is_multi": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "is_no_data": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "operand": {
              "Type": "String",
              "Required": true
            },
            "thresholds": {
              "Type": "List",
              "Required": true,
              "MaxItems": 1,
              "MinItems": 1,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "critical": {
                    "Type": "String",
                    "Optional": true,
                    "Default": {
                      "Type": "string"
                    }
                  },
                  "warning": {
                    "Type": "String",
                    "Optional": true,
                    "Default": {
                      "Type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "metric_query": {
        "Type": "List",
        "Required": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "display": {
              "Type": "String",
              "Optional": true
            },
            "exclude_filters": {
              "Type": "List",
              "Optional": true,
              "Description": "Not-equals filters (operand: neq)",
              "Computed": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "Map"
              }
            },
            "filters": {
              "Type": "List",
              "Optional": true,
              "Description": "Non-equality filters (operand: contains, regexp)",
              "Computed": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "Map"
              }
            },
            "final_window_operation": {
              "Type": "List",
              "Optional": true,
              "MaxItems": 1,
              "MinItems": 1,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "input_window_ms": {
                    "Type": "Int",
                    "Optional": true,
                    "Description": "Unit specified in milliseconds, but must be at least 30,000 and a round number of seconds (i.e. evenly divisible by 1,000)"
                  },
                  "operator": {
                    "Type": "String",
                    "Optional": true
                  }
                }
              }
            },
            "group_by": {
              "Type": "List",
              "Optional": true,
              "MaxItems": 1,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "aggregation_method": {
                    "Type": "String",
                    "Optional": true
                  },
                  "keys": {
                    "Type": "List",
                    "Optional": true,
                    "ConfigImplicitMode": "Attr",
                    "Elem": {
                      "Type": "SchemaElements",
                      "ElementsType": "String"
                    }
                  }
                }
              }
            },
            "hidden": {
              "Type": "Bool",
              "Required": true
            },
            "include_filters": {
              "Type": "List",
              "Optional": true,
              "Description": "Equality filters (operand: eq)",
              "Computed": true,
              "ConfigImplicitMode": "Attr",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "Map"
              }
            },
            "metric": {
              "Type": "String",
              "Optional": true,
              "Computed": true
            },
            "query_name": {
              "Type": "String",
              "Required": true
            },
            "spans": {
              "Type": "List",
              "Optional": true,
              "MaxItems": 1,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "group_by_keys": {
                    "Type": "List",
                    "Optional": true,
                    "ConfigImplicitMode": "Attr",
                    "Elem": {
                      "Type": "SchemaElements",
                      "ElementsType": "String"
                    }
                  },
                  "latency_percentiles": {
                    "Type": "List",
                    "Optional": true,
                    "Computed": true,
                    "ConfigImplicitMode": "Attr",
                    "Elem": {
                      "Type": "SchemaElements",
                      "ElementsType": "Float"
                    }
                  },
                  "operator": {
                    "Type": "String",
                    "Required": true
                  },
                  "operator_input_window_ms": {
                    "Type": "Int",
                    "Optional": true
                  },
                  "query": {
                    "Type": "String",
                    "Required": true
                  }
                }
              }
            },
            "timeseries_operator": {
              "Type": "String",
              "Optional": true,
              "Computed": true
            },
            "timeseries_operator_input_window_ms": {
              "Type": "Int",
              "Optional": true,
              "Description": "Unit specified in milliseconds, but must be at least 30,000 and a round number of seconds (i.e. evenly divisible by 1,000)"
            },
            "tql": {
              "Type": "String",
              "Optional": true,
              "Computed": true
            }
          }
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "type": {
        "Type": "String",
        "Computed": true
      }
    },
    "lightstep_metric_dashboard": {
      "chart": {
        "Type": "Set",
        "Optional": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "id": {
              "Type": "String",
              "Computed": true
            },
            "name": {
              "Type": "String",
              "Required": true
            },
            "query": {
              "Type": "List",
              "Required": true,
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "display": {
                    "Type": "String",
                    "Optional": true
                  },
                  "exclude_filters": {
                    "Type": "List",
                    "Optional": true,
                    "Description": "Not-equals filters (operand: neq)",
                    "Computed": true,
                    "ConfigImplicitMode": "Attr",
                    "Elem": {
                      "Type": "SchemaElements",
                      "ElementsType": "Map"
                    }
                  },
                  "filters": {
                    "Type": "List",
                    "Optional": true,
                    "Description": "Non-equality filters (operand: contains, regexp)",
                    "Computed": true,
                    "ConfigImplicitMode": "Attr",
                    "Elem": {
                      "Type": "SchemaElements",
                      "ElementsType": "Map"
                    }
                  },
                  "final_window_operation": {
                    "Type": "List",
                    "Optional": true,
                    "MaxItems": 1,
                    "MinItems": 1,
                    "IsBlock": true,
                    "ConfigImplicitMode": "Block",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "input_window_ms": {
                          "Type": "Int",
                          "Optional": true,
                          "Description": "Unit specified in milliseconds, but must be at least 30,000 and a round number of seconds (i.e. evenly divisible by 1,000)"
                        },
                        "operator": {
                          "Type": "String",
                          "Optional": true
                        }
                      }
                    }
                  },
                  "group_by": {
                    "Type": "List",
                    "Optional": true,
                    "MaxItems": 1,
                    "IsBlock": true,
                    "ConfigImplicitMode": "Block",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "aggregation_method": {
                          "Type": "String",
                          "Optional": true
                        },
                        "keys": {
                          "Type": "List",
                          "Optional": true,
                          "ConfigImplicitMode": "Attr",
                          "Elem": {
                            "Type": "SchemaElements",
                            "ElementsType": "String"
                          }
                        }
                      }
                    }
                  },
                  "hidden": {
                    "Type": "Bool",
                    "Required": true
                  },
                  "include_filters": {
                    "Type": "List",
                    "Optional": true,
                    "Description": "Equality filters (operand: eq)",
                    "Computed": true,
                    "ConfigImplicitMode": "Attr",
                    "Elem": {
                      "Type": "SchemaElements",
                      "ElementsType": "Map"
                    }
                  },
                  "metric": {
                    "Type": "String",
                    "Optional": true,
                    "Computed": true
                  },
                  "query_name": {
                    "Type": "String",
                    "Required": true
                  },
                  "spans": {
                    "Type": "List",
                    "Optional": true,
                    "MaxItems": 1,
                    "IsBlock": true,
                    "ConfigImplicitMode": "Block",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "group_by_keys": {
                          "Type": "List",
                          "Optional": true,
                          "ConfigImplicitMode": "Attr",
                          "Elem": {
                            "Type": "SchemaElements",
                            "ElementsType": "String"
                          }
                        },
                        "latency_percentiles": {
                          "Type": "List",
                          "Optional": true,
                          "Computed": true,
                          "ConfigImplicitMode": "Attr",
                          "Elem": {
                            "Type": "SchemaElements",
                            "ElementsType": "Float"
                          }
                        },
                        "operator": {
                          "Type": "String",
                          "Required": true
                        },
                        "operator_input_window_ms": {
                          "Type": "Int",
                          "Optional": true
                        },
                        "query": {
                          "Type": "String",
                          "Required": true
                        }
                      }
                    }
                  },
                  "timeseries_operator": {
                    "Type": "String",
                    "Optional": true,
                    "Computed": true
                  },
                  "timeseries_operator_input_window_ms": {
                    "Type": "Int",
                    "Optional": true,
                    "Description": "Unit specified in milliseconds, but must be at least 30,000 and a round number of seconds (i.e. evenly divisible by 1,000)"
                  },
                  "tql": {
                    "Type": "String",
                    "Optional": true,
                    "Computed": true
                  }
                }
              }
            },
            "rank": {
              "Type": "Int",
              "Required": true
            },
            "type": {
              "Type": "String",
              "Required": true
            },
            "y_axis": {
              "Type": "List",
              "Optional": true,
              "MaxItems": 1,
              "Deprecated": "The y_axis field is no longer used",
              "IsBlock": true,
              "ConfigImplicitMode": "Block",
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "max": {
                    "Type": "Float",
                    "Required": true
                  },
                  "min": {
                    "Type": "Float",
                    "Required": true
                  }
                }
              }
            }
          }
        }
      },
      "dashboard_name": {
        "Type": "String",
        "Required": true
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "type": {
        "Type": "String",
        "Computed": true
      }
    },
    "lightstep_pagerduty_destination": {
      "destination_name": {
        "Type": "String",
        "Required": true,
        "Description": "Lightstep destination name"
      },
      "integration_key": {
        "Type": "String",
        "Required": true,
        "Description": "PagerDuty Service Integration Key. To create one follow the docs here - https://support.pagerduty.com/docs/services-and-integrations#add-integrations-to-an-existing-service"
      },
      "project_name": {
        "Type": "String",
        "Required": true,
        "Description": "Lightstep project name"
      }
    },
    "lightstep_slack_destination": {
      "channel": {
        "Type": "String",
        "Required": true,
        "Description": "One of: slack channel name (#channel), channel ID, handle name (@user)."
      },
      "project_name": {
        "Type": "String",
        "Required": true,
        "Description": "Lightstep project name"
      }
    },
    "lightstep_stream": {
      "__timeouts__": [
        "create"
      ],
      "custom_data": {
        "Type": "List",
        "Optional": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "Map"
        }
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "query": {
        "Type": "String",
        "Required": true
      },
      "stream_name": {
        "Type": "String",
        "Required": true
      }
    },
    "lightstep_stream_condition": {
      "condition_name": {
        "Type": "String",
        "Required": true
      },
      "evaluation_window_ms": {
        "Type": "Int",
        "Required": true
      },
      "expression": {
        "Type": "String",
        "Required": true
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "stream_id": {
        "Type": "String",
        "Required": true
      }
    },
    "lightstep_stream_dashboard": {
      "dashboard_name": {
        "Type": "String",
        "Required": true
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "stream_ids": {
        "Type": "List",
        "Optional": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      }
    },
    "lightstep_webhook_destination": {
      "custom_headers": {
        "Type": "Map",
        "Optional": true,
        "Description": "Custom HTTP headers for the webhook request"
      },
      "destination_name": {
        "Type": "String",
        "Required": true,
        "Description": "Name of the webhook destination"
      },
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "template": {
        "Type": "String",
        "Optional": true,
        "Description": "Webhook payload body text template. Used for customing webhook messages"
      },
      "url": {
        "Type": "String",
        "Required": true,
        "Description": "Webhook URL"
      }
    }
  },
  "data-sources": {
    "lightstep_stream": {
      "project_name": {
        "Type": "String",
        "Required": true
      },
      "stream_id": {
        "Type": "String",
        "Required": true
      },
      "stream_name": {
        "Type": "String",
        "Computed": true
      },
      "stream_query": {
        "Type": "String",
        "Description": "Stream query",
        "Computed": true
      }
    }
  }
}